{
  "name": "hathor-red-music-platform",
  "version": "1.0.0",
  "description": "MVP music streaming platform with AI features",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "db:seed": "node database/seed.js",
    "db:generate-audio": "node database/generate-sample-audio.js",
    "db:setup": "npm run db:generate-audio && npm run db:seed"
  },
  "keywords": ["music", "streaming", "AI", "playlist"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.6.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
