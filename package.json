{
  "name": "hathor-red-music-platform",
  "version": "1.0.0",
  "description": "MVP music streaming platform with AI features",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "db:seed": "node database/seed.js",
    "db:generate-audio": "node database/generate-sample-audio.js",
    "db:setup": "npm run db:generate-audio && npm run db:seed",
    "sync:worker": "node -r ts-node/register server/sync/worker.ts"
  },
  "keywords": ["music", "streaming", "AI", "playlist"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@linear/sdk": "^68.1.0",
    "@notionhq/client": "^5.6.0",
    "bcryptjs": "^2.4.3",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.66.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
